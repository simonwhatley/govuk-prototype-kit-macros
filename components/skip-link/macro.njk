{% macro govukSkipLink(params) %}
<a href="{%- if params.href %}{{ params.href }}{% else %}#{% endif -%}" class="skiplink{%- if params.classes %} {{ params.classes }}{% endif -%}"
  {%- for attribute, value in params.attributes %} {{attribute}}="{{value}}"{% endfor %}>
	{{ (params.html | safe if params.html else (params.text if params.text else 'Skip to main content')) }}
</a>
{% endmacro %}